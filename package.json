  "devDependencies": {
    "@angular/compiler": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "@angular/compiler-cli": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "ng-packagr": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "typescript": "~5.4.0"
