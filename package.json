{
  "name": "ng-snowfall",
  "version": "1.0.1",
  "description": "Beautiful interactive snowfall component for Angular with mouse wind effect",
  "author": "Nikita Chicherin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Leksip/ng-snowfall.git"
  },
  "keywords": [
    "angular",
    "snowfall",
    "snow",
    "winter",
    "animation",
    "canvas",
    "interactive"
  ],
  "peerDependencies": {
    "@angular/common": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "@angular/core": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {},
  "main": "dist/ng-snowfall.umd.js",
  "module": "dist/ng-snowfall.es5.js",
  "es2015": "dist/ng-snowfall.es2015.js",
  "typings": "dist/index.d.ts",
